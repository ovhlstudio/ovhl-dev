{
	"name": "ovhl-tools",
	"version": "0.1.0",
	"description": "Enterprise Audit Tool for OVHL Framework",
	"main": "dist/index.js",
	"bin": {
		"ovhl": "./bin/ovhl.js",
		"ovhl-tools": "./bin/ovhl.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "node --no-warnings --loader ts-node/esm src/CLI/index.ts",
		"audit": "node --no-warnings --loader ts-node/esm src/CLI/index.ts audit",
		"start": "node bin/ovhl.js",
		"lint": "eslint src/**/*.ts",
		"format": "prettier --write src/**/*.ts"
	},
	"keywords": [
		"roblox",
		"framework",
		"audit",
		"cli",
		"ovhl"
	],
	"author": "OVHL Dev",
	"license": "ISC",
	"dependencies": {
		"@inquirer/prompts": "^8.0.1",
		"@toml-tools/parser": "^1.0.0",
		"boxen": "^5.1.2",
		"chalk": "^4.1.2",
		"cli-table3": "^0.6.3",
		"commander": "^11.1.0",
		"cosmiconfig": "^8.3.6",
		"fast-glob": "^3.3.2",
		"figlet": "^1.7.0",
		"fs-extra": "^11.2.0",
		"ora": "^5.4.1",
		"strip-json-comments": "^5.0.1"
	},
	"devDependencies": {
		"@types/figlet": "^1.5.8",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^20.10.0",
		"@typescript-eslint/eslint-plugin": "^6.13.1",
		"@typescript-eslint/parser": "^6.13.1",
		"concurrently": "^9.2.1",
		"eslint": "^8.54.0",
		"prettier": "^3.1.0",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.2"
	},
	"type": "module"
}
